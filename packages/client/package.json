{
  "name": "@triplit/client",
  "packageManager": "yarn@3.4.1",
  "version": "0.0.35",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "bin": {
    "triplit": "./dist/cli.cjs"
  },
  "scripts": {
    "build": "yarn build:code && yarn build:cli",
    "build:cli": "esbuild --bundle --platform=node --target=node14 --outfile=dist/cli.cjs cli/index.ts",
    "build:code": "yarn build:setup && parcel build . --no-cache && yarn build:cleanup",
    "build:setup": "mv tsconfig.json tsconfig.tmp.json && mv tsconfig.build.json tsconfig.json",
    "build:cleanup": "mv tsconfig.json tsconfig.build.json && mv tsconfig.tmp.json tsconfig.json",
    "lint": "yarn lint:code && yarn lint:cli",
    "lint:code": "tsc",
    "lint:cli": "cd ./cli && tsc",
    "publish-pkg": "node ../../scripts/npm-check-version-and-publish.js"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "@triplit/db": "workspace:^",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.8",
    "rxjs": "^7.8.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.9.3",
    "@parcel/transformer-typescript-tsc": "^2.9.3",
    "@types/node": "^20.4.0",
    "@types/prettier": "^2",
    "@types/yargs": "^17",
    "esbuild": "^0.17.19",
    "parcel": "^2.9.3",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
