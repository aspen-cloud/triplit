{
  "name": "@triplit/console",
  "version": "0.3.48",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn build:web && yarn build:server",
    "build:watch": "yarn build:web:watch & yarn build:server:watch",
    "build:web": "vite build --outDir dist/public",
    "build:web:watch": "vite build --outDir dist/public",
    "build:server": "yarn tsx build-server.ts",
    "build:server:watch": "yarn tsx watch build-server.ts",
    "build:types": "tsc -p tsconfig.server.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "publish-pkg": "node ../../scripts/npm-check-version-and-publish.js"
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.5.1",
    "@mantine/form": "^7.5.2",
    "@phosphor-icons/react": "^2.0.10",
    "@tanstack/react-table": "^8.9.8",
    "@triplit/client": "workspace:^",
    "@triplit/db": "workspace:^",
    "@triplit/react": "workspace:^",
    "@triplit/server-core": "workspace:^",
    "jotai": "^2.4.2",
    "lucide-react": "^0.276.0",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@triplit/ui": "workspace:^",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.19.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.5",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
