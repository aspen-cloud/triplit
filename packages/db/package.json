{
  "name": "@triplit/db",
  "version": "0.0.37",
  "source": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "yarn build:esm & yarn build:cjs & yarn build:types & wait",
    "build:esm": "tsc --module ESNext --outDir ./dist/esm",
    "build:cjs": "tsc --module CommonJS --outDir ./dist/cjs",
    "build:types": "tsc --emitDeclarationOnly",
    "lint:build": "npx publint",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "test-coverage-ui": "vitest --ui --coverage",
    "test-ci": "vitest run --coverage.enabled --coverage.reporter='text-summary'",
    "typecheck": "vitest typecheck",
    "bench": "vitest bench",
    "coverage": "stryker run",
    "publish-pkg": "node ../../scripts/npm-check-version-and-publish.js"
  },
  "files": [
    "/dist"
  ],
  "devDependencies": {
    "@stryker-mutator/core": "^7.1.1",
    "@stryker-mutator/vitest-runner": "^7.1.1",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "rxdb": "^14.14.2",
    "rxjs": "^7.8.1",
    "tinybench": "^2.5.0",
    "typescript": "^5.2.2",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.27.4",
    "ajv-formats": "^2.1.1",
    "nanoid": "^3.0.0",
    "pino": "^8.11.0",
    "sorted-btree": "^1.8.1",
    "tinybench": "^2.5.0",
    "tuple-database": "^2.2.0"
  }
}
